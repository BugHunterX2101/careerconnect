{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/services",
            "dest": "/public/services.html"
        },
        {
            "src": "/about",
            "dest": "/public/about.html"
        },
        {
            "src": "/contact",
            "dest": "/public/contact.html"
        },
        {
            "src": "/signup",
            "dest": "/public/signup.html"
        },
        {
            "src": "/dashboard",
            "dest": "/public/dashboard.html"
        },
        {
            "src": "/styles.css",
            "dest": "/public/styles.css"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "/public/$1"
        }
    ]
} 