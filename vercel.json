{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/register",
            "methods": ["POST"],
            "dest": "index.js"
        },
        {
            "src": "/api/login",
            "methods": ["POST"],
            "dest": "index.js"
        },
        {
            "src": "/api/test-db",
            "methods": ["GET"],
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
} 